# Define the data
prices <- c(1, 1, 5, 5, 5, 5, 5, 8, 8, 10, 10, 10, 10, 12, 14, 14, 14, 15, 15, 15, 15, 15, 15, 18, 18, 18, 18, 18)
# Mean
mean_price <- mean(prices)
mean_price

# Median
median_price <- median(prices)
median_price

# Mode (the most frequent value)
mode_price <- as.numeric(names(sort(table(prices), decreasing = TRUE)[1]))
mode_price

# Range
range_price <- range(prices)
range_price

# Frequency table
freq_table <- table(prices)
freq_table
# Histogram
hist(prices, main = "Histogram of Prices", xlab = "Price", ylab = "Frequency", col = "lightblue", breaks = 10)

# Boxplot
boxplot(prices, main = "Boxplot of Prices", ylab = "Price", col = "lightgreen")
